<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<record id="waction_base_agama" model="ir.actions.act_window">
			<field name="name">Agama</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">base.agama</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
		</record>
		
		<record id="WactView_base_treeAgama" model="ir.actions.act_window.view">
			<field eval="10" name="sequence"/>
			<field name="view_mode">tree</field>
			<field name="view_id" ref="tree_base_agama"/>
			<field name="act_window_id" ref="waction_base_agama"/>
		</record>

		<record id="WactView_base_formAgama" model="ir.actions.act_window.view">
			<field eval="20" name="sequence"/>
			<field name="view_mode">form</field>
			<field name="view_id" ref="form_base_agama"/>
			<field name="act_window_id" ref="waction_base_agama"/>
		</record>
	</data>
</openerp>
